def add_numbers(a, b):
    result = a + b
    return result

def multiply(x, y):
    return x * y

def divide(a, b):
    return a / b

class Calculator:
    def __init__(self):
        pass
    
    def calculate(self, operation, a, b):
        if operation == "add":
            return add_numbers(a, b)
        elif operation == "multiply":
            return multiply(a, b)
        elif operation == "divide":
            return divide(a, b)
        else:
            return None
